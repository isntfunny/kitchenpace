generator client {
  provider = "prisma-client"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
}

model Profile {
  id        String   @id @default(cuid())
  userId    String   @unique
  email     String
  nickname  String?
  teaser    String?
  photoUrl  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
