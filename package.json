{
    "name": "kitchenpace",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "panda": "panda codegen --config ./panda.config.ts",
        "predev": "npm run panda",
        "prebuild": "npm run panda && npx prisma generate",
        "postinstall": "npm run panda",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "prepare": "husky",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:seed": "tsx lib/seed.ts",
        "db:reset": "prisma migrate reset --force && npx prisma db push --accept-data-loss && npm run db:seed",
        "db:fresh": "npm run db:generate && npm run db:reset",
        "build:dev-seed": "DEBUG=1 npm run build && npm run db:fresh"
    },
    "prisma": {
        "seed": "tsx lib/seed.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.995.0",
        "@aws-sdk/lib-storage": "^3.995.0",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@pandacss/dev": "^1.8.2",
        "@prisma/adapter-pg": "^7.4.0",
        "@prisma/client": "^7.4.0",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-select": "^2.2.6",
        "@styled-system/css": "^5.1.5",
        "@xyflow/react": "^12.10.1",
        "bcrypt": "^6.0.0",
        "dagre": "^0.8.5",
        "dotenv": "^17.3.1",
        "next": "16.1.6",
        "next-auth": "^4.24.13",
        "nodemailer": "^7.0.13",
        "pg": "^8.18.0",
        "postcss": "^8.5.6",
        "prisma": "^7.4.0",
        "radix-ui": "^1.4.3",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "sharp": "^0.34.5",
        "smartcrop": "^2.0.5",
        "styled-system": "^5.1.5",
        "yet-another-react-lightbox": "^3.29.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/dagre": "^0.7.53",
        "@types/node": "20.19.33",
        "@types/nodemailer": "^7.0.11",
        "@types/pg": "^8.16.0",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.56.0",
        "eslint": "^9.39.2",
        "eslint-config-next": "^16.1.6",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-unused-imports": "^4.4.1",
        "husky": "^9.1.7",
        "prettier": "^3.8.1",
        "tsx": "^4.21.0",
        "typescript": "^5"
    }
}
